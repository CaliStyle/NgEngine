{
  "name": "ng-engine",
  "version": "6.0.0",
  "license": "MIT",
  "repository": "https://github.com/CaliStyle/NgEngine.git",
  "keywords": [
    "Angular",
    "Fabrix",
    "NgEngine",
    "Config"
  ],
  "author": "scott-wyatt",
  "homepage": "https://github.com/CaliStyle/ng-engine#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "run-s build:client build:aot build:server",
    "build:prod": "run-s build:client:prod build:aot:prod build:server",
    "build:staging": "run-s build:client:staging build:aot:staging build:server:staging",
    "build:testing": "run-s build:client:testing build:aot:testing build:server:testing",
    "build:client": "ng build",
    "build:client:prod": "ng build --prod --build-optimizer",
    "build:client:staging": "ng build --environment=staging",
    "build:client:testing": "ng build --environment=testing",
    "build:aot": "ng build --aot && ng run ng-engine:server",
    "build:aot:prod": "ng build --prod && ng run ng-engine:server",
    "build:aot:staging": "ng build --aot --target=staging --environment=staging && ng run ng-engine:server",
    "build:aot:testing": "ng build --aot --target=testing --environment=testing && ng run ng-engine:server",
    "build:server": "webpack",
    "build:server:staging": "webpack",
    "build:server:testing": "webpack",
    "build:server:prod": "webpack -p",
    "build:dist": "npm run clean && ng-packagr -p ng-package.json",
    "build:prod:ngsw": "npm run build:prod && npm run ngsw:config && npm run ngsw:copy",
    "serve:prod:ngsw": "npm run build:prod:ngsw && node dist/server",
    "ngsw:config": "node_modules/.bin/ngsw-config dist src/ngsw-config.json",
    "ngsw:copy": "cp node_modules/@angular/service-worker/ngsw-worker.js dist/",
    "test": "run-s test:ng test:e2e test:server",
    "test:ng": "ng test --watch=false",
    "test:e2e": "ng e2e",
    "test:ng:watch": "ng test",
    "test:server": "npm run build && mocha",
    "lint": "eslint --fix . && ng lint --fix .",
    "e2e": "ng e2e",
    "heroku-postbuild": "npm run build:prod",
    "clean": "rm -rf dist"
  },
  "main": "./dist/ng-engine.js",
  "peerDependencies": {
    "@angular/core": "6.0.7",
    "@angular/router": "6.0.7",
    "lodash": "^4.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/animations": "6.0.7",
    "@angular/cdk": "^6.3.2",
    "@angular/cli": "6.0.8",
    "@angular/common": "6.0.7",
    "@angular/compiler": "6.0.7",
    "@angular/compiler-cli": "6.0.7",
    "@angular/core": "6.0.7",
    "@angular/forms": "6.0.7",
    "@angular/http": "6.0.7",
    "@angular/language-service": "6.0.7",
    "@angular/material": "^6.3.2",
    "@angular/platform-browser": "6.0.7",
    "@angular/platform-browser-dynamic": "6.0.7",
    "@angular/platform-server": "6.0.7",
    "@angular/router": "6.0.7",
    "@angular/service-worker": "6.0.7",
    "@fabrix/fabrix": "^1.0.3",
    "@fabrix/spool-express": "^1.0.0",
    "@fabrix/spool-repl": "^1.0.0",
    "@fabrix/spool-router": "^1.0.0",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/entity": "^6.0.1",
    "@ngrx/router-store": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~10.3.4",
    "@types/winston": "^2.2.20-alpha",
    "codelyzer": "^4.0.1",
    "core-js": "^2.4.1",
    "express": "^4.16.2",
    "hammerjs": "^2.0.8",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^3.0.2",
    "ng-packagr": "^2.2.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.1.2",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.2.1",
    "supertest": "^3.0.0",
    "ts-loader": "^4.4.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.7.1",
    "webpack-cli": "^2.1.3",
    "webpack-node-externals": "^1.6.0",
    "winston": "^2.2",
    "zone.js": "^0.8.26"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 3"
  }
}
